{
    "name": "araise/table-bundle",
    "type": "symfony-bundle",
    "description": "Table generator for symfony",
    "license": "MIT",
    "homepage": "https://araise.dev",
    "minimum-stability": "stable",
    "keywords": [
        "symfony-ux"
    ],
    "authors": [
        {
            "name": "whatwedo GmbH",
            "email": "welove@whatwedo.ch"
        }
    ],
    "require": {
        "php":                      ">=8.1",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/http-kernel":      "^5.4|^6.0",
        "symfony/validator":        "^5.4|^6.0",
        "araise/core-bundle":     "^1.0",
        "araise/search-bundle":   "^3.0",
        "phpoffice/phpspreadsheet": "^1.22",
        "symfony/stimulus-bundle": "^2.10"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^v6.0",
        "symfony/http-kernel": "^v6.0",
        "symfony/config": "^v6.0",
        "symfony/dependency-injection": "^v6.0",
        "symfony/yaml": "^v6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/validator": "^6.0",
        "nyholm/symfony-bundle-test": "^1.8",
        "doctrine/doctrine-bundle": "2.5.5",
        "doctrine/orm": "^2.11",
        "whatwedo/php-coding-standard": "^1.0",
        "zenstruck/foundry": "^1.16",
        "zenstruck/console-test": "^v1.1.0",
        "symfony/orm-pack": "^1.0",
        "symfony/translation": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "gedmo/doctrine-extensions": "^3.5",
        "symfony/webpack-encore-bundle": "^1.13",
        "symfony/security-core": "^6.0",
        "symfony/security-bundle": "^6.0",
        "phpstan/phpstan": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "araise\\TableBundle\\": "src/",
            "araise\\TableBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-feature/query-builder-provider": "0.4.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
